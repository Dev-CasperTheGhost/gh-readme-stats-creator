{
  "name": "remix-app",
  "version": "0.0.0",
  "license": "MIT",
  "description": "Create a GitHub README stat card with ease!",
  "scripts": {
    "clean": "rimraf ./node_modules/.cache ./.cache ./server/dist ./build ./public/build",
    "prebuild": "npm run clean && echo All clean âœ¨",
    "watch:css": "postcss app/styles/tailwind-entry.css --output app/styles/tailwind.css -w",
    "build:css": "postcss app/styles/tailwind-entry.css --output app/styles/tailwind.css --env production",
    "dev:remix": "remix dev",
    "dev": "concurrently \"npm run watch:css\" \"npm run dev:remix\"",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,css,json}\" --ignore-path .gitignore",
    "lint": "eslint . --ext .ts,.js,.tsx,.jsx,",
    "postinstall": "npx remix setup node",
    "build:remix": "remix build",
    "build": "npm run build:css && npm run build:remix",
    "start": "remix-serve api/build"
  },
  "dependencies": {
    "@react-aria/focus": "^3.5.0",
    "@react-aria/label": "^3.2.1",
    "@react-aria/ssr": "^3.1.0",
    "@react-aria/switch": "^3.1.3",
    "@react-aria/utils": "^3.10.0",
    "@react-aria/visually-hidden": "^3.2.3",
    "@react-stately/toggle": "^3.2.3",
    "@remix-run/react": "^1.0.6",
    "@remix-run/serve": "^1.0.6",
    "classnames": "^2.3.1",
    "react": "^17.0.2",
    "react-bootstrap-icons": "^1.6.1",
    "react-cool-onclickoutside": "^1.7.0",
    "react-dom": "^17.0.2",
    "remix": "^1.0.6",
    "zustand": "^3.6.6"
  },
  "devDependencies": {
    "@casper124578/eslint-config": "^2.2.0",
    "@casper124578/eslint-config-react": "^2.2.0",
    "@remix-run/dev": "^1.0.6",
    "@remix-run/server-runtime": "^1.0.6",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "autoprefixer": "^10.4.0",
    "concurrently": "^6.4.0",
    "eslint": "^8.4.0",
    "postcss": "^8.4.4",
    "postcss-cli": "^9.0.2",
    "prettier": "^2.5.1",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.5.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}